{
    "name": "hadeluca/db-exporter",
    "description": "this is a package for Laravel Framework to export, via command line, Database in Migration and all data in a Seed",
    "keywords":["Export","PHP","Database","Migrations","Laravel","Seed","Command","Artisan"],
    "require": {
        "symfony/console": "~5.0",
        "doctrine/inflector": "~2.0",
        "paragonie/random_compat": "v9.99.99",
        "illuminate/support": "^8.40",
        "illuminate/console": "^8.40",
        "illuminate/container": "^8.40",
        "illuminate/contracts": "^8.40",
        "illuminate/database": "^8.40",
        "maatwebsite/excel": "~3.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Hernan Ariel De Luca",
            "email": "hadeluca@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hardel\\": "src/Hardel/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hardel\\Exporter\\DBExporterServiceProvider"
            ]
        }
    },
    "prefer-stable":true
}
